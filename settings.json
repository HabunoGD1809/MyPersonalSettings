  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.suggestSelection": "first",
  "files.insertFinalNewline": true, //modifique esto para que no se agregue un salto de linea al final de un archivo
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "kite.showWelcomeNotificationOnStartup": false,
  "settings.cycle": [
    {
      "id": "lineNumbers",
      "values": [
        {
          "editor.lineNumbers": "off", //cambie esto
          "editor.folding": false,
          "editor.glyphMargin": false
        },
        {
          "editor.lineNumbers": "on",
          "editor.folding": true,
          "editor.glyphMargin": true
        }
      ]
    }
  ],
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "editor.cursorStyle": "line",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.autoIndent": "brackets",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "on",
  "vsicons.dontShowNewVersionMessage": true,
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.gpuAcceleration": "off",
  "files.autoSave": "afterDelay",
  "redhat.telemetry.enabled": true,
  "vs-kubernetes": {
    "vscode-kubernetes.kubectl-path.linux": "/home/habuno_0fj/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path.linux": "/home/habuno_0fj/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vscode-kubernetes.helm-path.linux": "/home/habuno_0fj/.vs-kubernetes/tools/helm/linux-amd64/helm",
